# Flameshot
shift + Print
	flameshot gui

Print
	flameshot full -c

ctrl + Print
	flameshot launcher

# suckless specifics
super + space
	dmenu_run

shift + super + equal
	= --dmenu=dmenu

# some apps
super + e
	alacritty --config-file=/home/jazze/.config/alacritty/alacritty-norm.toml --title=LF --command=$HOME/.config/lf/lfub

super + Return
	alacritty

super + a
	alacritty --config-file=$HOME/.config/alacritty/alacritty-norm.toml

ctrl + o
	exec $HOME/.config/lf/lfub

ctrl + up
    pactl set-sink-volume @DEFAULT_SINK@ +5%

ctrl + down
	pactl set-sink-volume @DEFAULT_SINK@ -5%
